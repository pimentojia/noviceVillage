{
  "entryChunk": {
    "index": "./src/index.js"
  },
  "template": [
    {
      "path": "./public/index.html",
      "entry": "index"
    }
  ],
  "vars": {
    "importLayout": false
  },
  "alias": {
    "~": "./src"
  },
  "proxy": [{
    "context": ["/tenant/**", "/api/**","/notify/**","/frontend/**"],
    "target": "http://192.168.0.61:8080",
    "secure": false
  }],
  "commonChunk": {
    "vendor": [
      "react",
      "react-dom",
      "react-router",
      "mobx"
    ]
  },
  "extraBabelPlugins": [
    ["import", [
      {
        "libraryName": "@uyun/uyd",
        "style": true
      }
    ]],
    ["@uyun/everest-i18n/babel", {
      "locales": "./src/common/locales.json",
      "translators": ["__"]
    }]
  ]
}
